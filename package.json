    "eslint-config-prettier": "8.8.0",
    "file-loader": "6.2.0",
    "html-loader": "4.2.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.7.5",
    "prettier": "2.8.7",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.61.0",
    "sass-loader": "13.2.2",
    "ts-loader": "9.4.2",
    "typescript": "5.0.2",
    "webpack": "5.78.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.13.2",
    "webpack-merge": "5.8.0"
